# Railway Production Environment Variables
# Copy these to your Railway project environment settings

# Flask Configuration
FLASK_ENV=production
FLASK_DEBUG=False
SECRET_KEY=your-production-secret-key-here

# Database (PostgreSQL on Railway)
DATABASE_URL=${DATABASE_URL}

# External APIs
OPENAI_API_KEY=your-openai-key-here
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_SECRET_KEY=your-stripe-secret-key

# CORS Origins
CORS_ORIGINS=https://astroyorumai.com,https://www.astroyorumai.com,http://localhost:8080

# Railway Specific
PORT=${PORT}
RAILWAY_ENVIRONMENT=production

# Application Settings
APP_NAME=AstroYorumAI
APP_VERSION=2.1.3-production
CALCULATION_METHOD=flatlib Swiss Ephemeris